{
    "name": "valpuia/mizokhua",
    "description": "List of districts and villages in Mizoram",
    "keywords": [
        "Valpuia",
        "laravel",
        "mizokhua",
        "mizoram",
        "mizoram district",
        "mizoram village"
    ],
    "homepage": "https://github.com/valpuia/mizokhua",
    "license": "MIT",
    "authors": [
        {
            "name": "Valpuia",
            "email": "valpuia604@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2||^8.3||^8.4",
        "spatie/laravel-package-tools": "^1.16||^2.0||^3.0",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.10.0||^8.1.1||^9.0",
        "orchestra/testbench": "^9.0.0||^10.0||^11.0||^12.0",
        "pestphp/pest": "^2.34||^3.0||^4.0",
        "pestphp/pest-plugin-arch": "^2.7||^3.0",
        "pestphp/pest-plugin-laravel": "^2.3||^3.0||^4.0"
    },
    "autoload": {
        "psr-4": {
            "Valpuia\\Mizokhua\\": "src/",
            "Valpuia\\Mizokhua\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valpuia\\Mizokhua\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-mizokhua --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Valpuia\\Mizokhua\\MizokhuaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}